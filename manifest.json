{
  "name": "Vergil+",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Vergil could be better.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://vergil.registrar.columbia.edu/*"
  ],
  "web_accessible_resources": [
   "js/VES_0.0.1.js",
   "js/gVES_0.0.1.js"
  ]
}
